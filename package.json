{
    "name": "mern-stack-loginauth",
    "version": "1.0.0",
    "description": "login-auth with the mern-stack",
    "main": "server.js",
    "scripts": {
        "client-install": "npm install --prefix client",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kal-J/MERN-stack-loginAuth.git"
    },
    "keywords": [
        "mern",
        "mongodb",
        "express",
        "react",
        "node",
        "js"
    ],
    "author": "kal J",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kal-J/MERN-stack-loginAuth/issues"
    },
    "homepage": "https://github.com/kal-J/MERN-stack-loginAuth#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "concurrently": "^4.1.0",
        "cors": "^2.8.5",
        "express": "^4.17.0",
        "is-empty": "^1.2.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.7.5",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "react-redux": "^7.0.3",
        "redux": "^4.0.1",
        "validator": "^10.11.0"
    },
    "devDependencies": {
        "nodemon": "^1.18.10"
    }
}